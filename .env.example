# Shopify Store Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to git

# Store domain
SHOPIFY_STORE_DOMAIN=healthfood-juicebar100.myshopify.com

# Admin API Token (get from Shopify Admin → Apps → Manage private apps)
SHOPIFY_ADMIN_API_TOKEN=shpat_your_token_here

# App Credentials (if building a custom app)
SHOPIFY_API_KEY=your_api_key_here
SHOPIFY_API_SECRET=your_api_secret_here

# Development settings
SHOPIFY_CLI_THEME_TOKEN=your_theme_token_here
SHOPIFY_PASSWORD=your_store_password_here

# GitHub Token (for CI/CD)
GITHUB_TOKEN=ghp_your_token_here